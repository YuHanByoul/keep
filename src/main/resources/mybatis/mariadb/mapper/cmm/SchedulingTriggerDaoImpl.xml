<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!--
    SQL File Name : SchedulingTriggerDaoImpl.xml
    Description : 스케쥴링트리거
-->
<mapper namespace="com.kbrainc.plum.cmm.scheduling.model.SchedulingTriggerDao">    

    <!-- 스케줄링트리거정보 목록을 조회한다. -->
    <select id="selectSchedulingTriggerList" resultType="SchedulingTriggerVo">
        /* SchedulingTriggerDao.selectSchedulingTriggerList */
        SELECT TRIGGERID
              ,NM
              ,DC
              ,CRON_EXPRESSION
              ,USE_YN
          FROM TB_CMM_SCHED_TRIGGER
         WHERE USE_YN = 'Y'
    </select>
</mapper>