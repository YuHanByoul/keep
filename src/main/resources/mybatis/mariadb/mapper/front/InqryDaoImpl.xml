	<!-- 1:1문의답변 등록 -->
	<insert id="insertInqry" parameterType="front.InqryVo">
		/* InqryDao.insertInqry */
		INSERT INTO TB_CMM_INQRY(
		        TITLE
		        ,CNTNTS
		        ,USER_NM
		        ,EMAIL
		        ,MDFCN_DT
		        ,MDFRID
		        ,REG_DT
		        ,RGTRID
		    )VALUES(
                 #{title       }
                 ,#{cntnts     }
                 ,#{nm         }
                 ,#{email      }
                 ,NOW()
                 ,#{user.userid}
                 ,NOW()
                 ,#{user.userid}
            ) 
	</insert>
	
