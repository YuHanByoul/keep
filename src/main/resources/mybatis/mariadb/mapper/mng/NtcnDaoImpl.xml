<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!--
    SQL File Name : NtcnDaoImpl.xml
    Description : 알림 등록용
-->

<mapper namespace="com.kbrainc.plum.mng.ntcn.model.NtcnDao">

    <!-- 교육일정 관리 게시글 등록 --> 
    <insert id="insertNtcn" parameterType="NtcnVo" useGeneratedKeys="true" keyProperty="ntcnid">
        /* NtcnDao.insertNtcn */
        INSERT INTO TB_CMM_NTCN(
            USERID
             ,KND_CD
             ,TTL
             ,CN
             ,MVMNURL
             ,INQ_YN
             ,REG_DT
        )VALUES(
            #{userid }
            ,#{kndCd  }
            ,#{ttl    }
            ,#{cn     }
            ,#{mvmnurl}
            ,#{inqYn }
            ,NOW() 
        )
    </insert>
</mapper>